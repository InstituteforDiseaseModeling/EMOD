#define Sample_Rate_0_18mo_DESC_TEXT     "Relative sampling rate for infants for the 0-18-month age group for age-adapted sampling."
#define Sample_Rate_10_14_DESC_TEXT     "Relative sampling rate for children in the age group 10-14 years for age-adapted sampling."
#define Sample_Rate_15_19_DESC_TEXT     "Relative sampling rate for adolescents in the age group 15-19 years for age-adapted sampling."
#define Sample_Rate_18mo_4yr_DESC_TEXT     "Relative sampling rate for children in the age group 18 months to 4 years for age-adapted sampling."
#define Sample_Rate_20_Plus_DESC_TEXT     "Relative sampling rate for young adults in the age group 20 years or greater for age-adapted sampling."
