                "Intervention_Config": {
                    "class": "MigrateFamily",
                    "NodeID_To_Migrate_To": 1,
                    "Duration_Before_Leaving_Distribution_Type" : "FIXED_DURATION",
                    "Duration_Before_Leaving_Fixed": 0, 
                    "Duration_At_Node_Distribution_Type" : "FIXED_DURATION",
                    "Duration_At_Node_Fixed": 10, 
                    "Is_Moving" : 0
                }
            }
        },
        {
